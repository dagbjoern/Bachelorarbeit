# pgf-Backend benutzen
backend : pgf

# Größe der Zeichenfläche in Zoll
figure.figsize : 7, 5.5

# Serifenfont benutzen
font.family : serif

# Fontgrößen
font.size : 13
legend.fontsize : medium
xtick.labelsize : 10
ytick.labelsize : 10

# unsere Fonteinstellungen benutzen
pgf.rcfonts : False

# TeX benutzen
text.usetex : True
pgf.texsystem : lualatex
